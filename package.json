{
  "name": "tvws",
  "version": "0.0.6",
  "type": "module",
  "description": "Browser-compatible TradingView WebSocket API with multiple endpoints support",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/badsector666/tvws.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "README.MD"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outfile dist/index.js --minify --target browser --format esm && tsc --emitDeclarationOnly --declarationMap false",
    "build:dev": "tsc",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run typecheck && bun run build"
  },
  "author": "badsector666 <badsectorkiller666@gmail.com>",
  "license": "MIT",
  "keywords": [
    "tradingview",
    "scraper",
    "scraping",
    "websocket",
    "candlestick",
    "browser",
    "cdn",
    "es2015",
    "market-data",
    "forex",
    "stocks",
    "crypto"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^17.0.45",
    "typescript": "^4.9.5"
  }
}
