{
  "name": "tvws",
  "version": "0.0.12",
  "module": "ESNext",
  "description": "Browser-compatible TradingView WebSocket API with extensible event system",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/badsector666/tvws.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "browser": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "bun run build.js",
    "build:types": "bunx tsc --emitDeclarationOnly",
    "typecheck": "bunx tsc --noEmit",
    "dev": "bun run build",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run typecheck && bun run build"
  },
  "author": "badsector666 <badsectorkiller666@gmail.com>",
  "license": "MIT",
  "keywords": [
    "tradingview",
    "websocket",
    "market-data",
    "candlestick",
    "browser",
    "cdn",
    "es2015",
    "forex",
    "stocks",
    "crypto",
    "real-time",
    "api",
    "extensible"
  ],
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
